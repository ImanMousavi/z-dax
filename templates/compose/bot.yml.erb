version: '3.6'

services:
  z-maker:
    image: "<%= @config['image']['z-maker'] %>"
    networks:
      - backend-net
      - gateway-net
    configs:
      - source: strategy_config
        target: /home/app/config/strategy.yaml
    command: start

configs:
  strategy_config:
    file: ../config/z-maker/strategy.yaml

networks:
  backend-net:
    external: true
  gateway-net:
    external: true
